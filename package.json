    "ngx-countdown-timer": "^0.1.9",
    "ngx-pagination": "^3.1.1",
    "ngx-quill": "^3.6.0",
    "node-sass": "^4.12.0",
    "quill": "^1.3.6",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.5.2",
    "scss-bundle": "^2.5.1",
    "suku-icons-library": "git+https://parashur:d053b93a67bdbfa89b1d6b79a0de35540d644dd3@github.com/SukuLab/suku-icons-library.git#feature/svg-updates-v1",
