{
  "name": "suku-lib",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build suku-webcomponents",
    "build:lib:schematics": "tsc -p ./projects/suku-webcomponents/tsconfig.schematics.json",
    "build:lib:scss": "scss-bundle -c scss-bundle.config.json",
    "copy:collection": "copyfiles --flat projects/suku-webcomponents/collection.json dist/suku-webcomponents",
    "build:package": "npm run build:lib && npm run build:lib:scss && npm run copy:collection",
    "gen-icon": "icon-font-generator ./src/assets/icons/*.svg -o ./src/assets/suku-icons -n=suku -p=suku"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.4",
